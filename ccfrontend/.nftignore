node_modules/@mui/icons-material/**/*.mjs
node_modules/@mui/icons-material/**/*.json
node_modules/@mui/icons-material/**/README.md
node_modules/@mui/icons-material/**/LICENSE

# Development files
node_modules/**/*.test.js
node_modules/**/*.spec.js
node_modules/**/*.d.ts.map
node_modules/**/*.ts.map
node_modules/**/.git
node_modules/**/.github

# Large packages that aren't needed in trace
node_modules/typescript
node_modules/eslint
node_modules/@typescript-eslint
node_modules/prettier
node_modules/storybook
node_modules/@storybook
